<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="timberDebug" format="color" />
    <attr name="timberError" format="color" />
    <attr name="timberWarn" format="color" />

    <declare-styleable name="CheckablePreferenceView">
        <attr name="checkableType" format="enum">
            <enum name="checkbox" value="0" />
            <enum name="switch_compat" value="1" />
        </attr>
        <attr name="disabledDescriptionText" format="string" />
    </declare-styleable>

    <declare-styleable name="RefreshLayout">
        <attr name="scrollingChildId" format="reference" />
        <attr name="spinnerBackground" format="color" />
        <attr name="spinnerColors" format="reference" />
    </declare-styleable>

    <declare-styleable name="TournamentTabsView">
        <attr name="indicatorLineColor" format="color" />
    </declare-styleable>

    <declare-styleable name="View">
        <attr name="descriptionText" format="string" />
        <attr name="drawableTintColor" format="color" />
        <attr name="icon" format="reference" />
        <attr name="image" format="reference" />
        <attr name="titleText" format="string" />
    </declare-styleable>

</resources>

<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="timberDebug" format="color" />
    <attr name="timberError" format="color" />
    <attr name="timberWarn" format="color" />

    <declare-styleable name="CheckablePreferenceView">
        <attr name="checkableType" format="enum">
            <enum name="checkbox" value="0" />
            <enum name="switch_compat" value="1" />
        </attr>
        <attr name="disabledDescriptionText" format="string" />
    </declare-styleable>

    <declare-styleable name="NoContentLinearLayout">
        <attr name="imageSrc" format="reference" />
    </declare-styleable>

    <declare-styleable name="RefreshLayout">
        <attr name="scrollingChild" format="reference" />
        <attr name="spinnerBackground" format="color" />
        <attr name="spinnerColors" format="reference" />
    </declare-styleable>

    <declare-styleable name="SimplePreferenceView">
        <attr name="simpleIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="View">
        <attr name="descriptionText" format="string" />
        <attr name="drawableTintColor" format="color" />
        <attr name="titleText" format="string" />
    </declare-styleable>

    <declare-styleable name="WinsLossesColumnView">
        <attr name="playerOrOpponent" format="enum">
            <enum name="player" value="0" />
            <enum name="opponent" value="1" />
        </attr>
    </declare-styleable>

</resources>

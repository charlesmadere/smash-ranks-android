<?xml version="1.0" encoding="utf-8"?>
<com.garpr.android.views.RatingItemView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    style="@style/ListItem">
    <TextView
        android:id="@+id/tvRating"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textColor="?android:attr/textColorPrimary"
        android:textSize="@dimen/text_size_xlarge"
        tools:text="Rating: 39.101" />
    <TextView
        android:id="@+id/tvUnadjusted"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        tools:text="Unadjusted: 45.679 (+/- 2.193)" />
</com.garpr.android.views.RatingItemView>
